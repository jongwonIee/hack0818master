<h1>사요</h1>

<div id=outer>

search
<%= form_tag posts_path, :method => 'get' do %>
    <p>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", :name => nil %>
    </p>
<% end %>

<%= link_to "상세 검색", new_search_path %>

<br>
전체 글
<br>
<% @posts.each do |p| %>
<div id=inner>
    <%= p.title %> | <%=link_to '이 글 보기', post_path(p.id)%> | <%=link_to '수정하기', edit_post_path(p)%>
    | <%= link_to '삭제 하기', p, method: :delete, data: { confirm: '정말 지우시겠습니까?' } %> 
    </div>
    <br>
<% end %>

